version 43
__lua__
function _init()
	print("hello world")
	position = 60
	current_time = 0.0
	previous_time = 0.0
	cooldown = 0.3
end

function _update()
	current_time = time()
	
	if current_time - previous_time > cooldown then
		if (btn(⬅️)) then
			position -= 4
			current_time = 0
			previous_time = time()
		elseif (btn(➡️)) then
			position += 4
			current_time = 0
			previous_time = time()
		end
		current_time = 0
		previous_time = time()
	end
end

function _draw()
	cls()
	rrectfill(0, 0, 128, 128, 3)
	spr(4, position, 60)
	sspr(0, 0, 8, 8, 40, 20, 2, 80)
	sspr(0, 0, 8, 8, 40, 100, 42, 2)
	sspr(0, 0, 8, 8, 80, 20, 2, 80)
end
